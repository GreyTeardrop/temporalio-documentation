{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "dependencyDashboard": true,
  "labels": [
    "dependencies"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "^docs/.*\\.md$"
      ],
      "matchStrings": [
        "(?<depName>temporalio/[a-z-]+):(?<currentValue>[0-9.]+)"
      ],
      "datasourceTemplate": "docker"
    }
  ]
}
